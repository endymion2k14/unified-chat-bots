{
  "console": {
    "port": 8080
  },
  "discord": [
    {
      "name": "name.for.in.logs",
      "enabled": true,
      "token": "discord.token"
    }
  ],
  "twitch": [
    {
      "name": "name.for.in.logs",
      "enabled": true,
      "secrets": {
        "token": "bot.token",
        "usertoken": "automatically.filled.in",
        "id": "bot.id.optional",
        "secret": "bot.secret.optional",
        "refresh": "automatically.filled.in",
        "expiry": 0,
        "redirectUri": "bot.oauth.url.callback",
        "scopes": "chat:read chat:edit channel:moderate moderator:read:followers clips:edit moderator:manage:announcements"
      },
      "settings": {
        "channel": "streamer.name",
        "username": "bot.username",
        "prefix": "command.prefix.!.optional",
        "chat_show": true,
        "chat_delay": 0,
        "chat_silence": false,
        "token_refresh": false,
        "eventsub": false,
        "superusers" : [
          "streamer.or.dev.or.maintainer.username.goes.here.optional"
        ],
        "usersIgnore": [
          "sery_bot",
          "soundalerts",
          "optional"
        ],
        "commandsIgnore": [
          "optional"
        ],
        "systemsIgnore": [
          "optional"
        ],
        "commandsOffline": [
          "uptime"
        ]
      }
    }
  ],
  "obs": [
    {
      "name": "obs.bot.name",
      "enabled": true,
      "settings": {
        "host": "localhost",
        "port": 4455,
        "password": "optional.obs.password"
      }
    }
  ]
}
