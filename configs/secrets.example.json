{
  "console": {
    "port": 8080
  },
  "discord": [
    {
      "name": "name.for.in.logs",
      "enabled": true,
      "token": "discord.token"
    }
  ],
  "twitch": [
    {
      "name": "name.for.in.logs",
      "enabled": true,
        "secrets": {
         "appToken": "bot.token",
         "clientId": "bot.id.optional",
         "clientSecret": "bot.secret.optional",
         "botToken": "automatically.filled.in",
         "botRefresh": "automatically.filled.in",
         "botExpiry": 0,
         "broadcasterToken": "automatically.filled.in",
         "broadcasterRefresh": "automatically.filled.in",
         "broadcasterExpiry": 0,
         "redirectUri": "bot.oauth.url.callback",
         "scopes": "chat:read chat:edit channel:moderate moderator:read:followers clips:edit moderator:manage:announcements"
       },
      "settings": {
        "channel": "streamer.name",
        "username": "bot.username",
        "prefix": "command.prefix.!.optional",
        "chat_show": true,
        "chat_delay": 0,
        "chat_silence": false,
        "token_refresh": false,
        "eventsub": false,
        "ircTokenSource": "app",
        "superusers" : [
          "streamer.or.dev.or.maintainer.username.goes.here.optional"
        ],
        "usersIgnore": [
          "sery_bot",
          "soundalerts",
          "optional"
        ],
        "commandsIgnore": [
          "clip",
          "setgame",
          "settitle",
          "optional"
        ],
        "systemsIgnore": [
          "optional"
        ],
        "commandsOffline": [
          "test",
          "uptime"
        ]
      }
    }
  ],
  "obs": [
    {
      "name": "obs.bot.name",
      "enabled": true,
      "settings": {
        "host": "localhost",
        "port": 4455,
        "password": "optional.obs.password"
      }
    }
  ]
}
